    <script>
      $(document).ready(function() {
        // the body of this function is in assets/js/now-ui-kit.js

        //for audio play




       var myVar = <%- JSON.stringify(databasemusic) %>;

      let idValues = myVar.map((items) => {
        return items.id;
      });
    //console.log(idValues)


     //console.log(check);


$(".play").on("click", function(){

let check = $(this).attr("id");
console.log(check);

       idValues.map((ids)=> {
         let aud = $( "#" + ids + "")[0];
         if(check === ids){
            // begin of this if
     if(aud.paused){

     aud.play();
     $(this).text("Pause");

    } else {
           aud.pause();
        $(this).text("Play");
    }
    // end of if
    aud.ontimeupdate = function() {
    $("."+ check + "").css("width", aud.currentTime / aud.duration * 100 + "%");
    $("."+ check + "").css("background-color", "black");
      $("."+ check + "").css("height", 7);
  }

  $("#reload").on("click", function(){
    aud.currentTime = 0;
  });

}

});
});



